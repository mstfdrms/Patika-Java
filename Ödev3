import java.util.Scanner;

public class a {

    public static void main(String[] args) {
        int a,b;
        double c;

        Scanner inp = new Scanner(System.in);
        System.out.println("birinc kenar:");
        a= inp.nextInt();
        System.out.println("ikinci kenar:");
        b= inp.nextInt();

        c=Math.sqrt((a*a)+(b*b));

        System.out.println(c);

    }
}
